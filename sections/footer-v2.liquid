

{% comment %} Add padding bottom for toolbar mobile {% endcomment %}
{% liquid
  assign placeholder = 'newsletter.email' | t
  assign template = template | split: '.' | first
  if template != 'product'
    assign pb_mobile = '--pb-mobile: 53px'
  else
    assign pb_mobile = ''
  endif
%}

{% comment %} Sticky add cart is hidden when reach here {% endcomment %}
<xo-sticky-hidden xo-name='sticky-addcart'></xo-sticky-hidden>
<footer {% render 'section-attr', style: pb_mobile %}>
  {% render 'section-bg-image' %}
  <div class='footer-v18__top'>
    <xo-container {% render 'section-layout' %}>
      <xo-grid>
        {% for block in section.blocks %}
          {% case block.type %}
            {% when 'information' %}
              <xo-animate xo-cascade style='--xs: 12; --lg: 3'>
                {%- if block.settings.logo != blank -%}
                  <a href='/' class='footer-v18__logo'>
                    {%- assign logo_alt = block.settings.logo.alt | default: shop.name | escape -%}
                    {%- assign width = block.settings.logo_width %}
                    {%- assign height = block.settings.logo_width | divided_by: block.settings.logo.aspect_ratio %}
                    <img
                      src='{{ block.settings.logo | image_url }}'
                      alt='{{ logo_alt }}'
                      style='width: {{ width }}px; height: {{ height }}px'
                      height='auto'
                      width='auto'
                    >
                  </a>
                {%- endif -%}
                <div class='footer-v18__description'>
                  {{ block.settings.description }}
                </div>
                {%- if block.settings.newsletter_enable -%}
                  {%- form 'customer', id: 'ContactFooter' -%}
                    <input type='hidden' name='contact[tags]' value='newsletter'>
                    <div class='footer-v18__input-btn'>
                      {% assign value = form.email %}
                      {% capture attr %}
                              aria-required='true'
                              autocorrect='off'
                              {% if form.errors %}
                                autofocus
                                aria-invalid='true'
                                aria-describedby='ContactFooter-error'
                              {% elsif form.posted_successfully? %}
                                aria-describedby='ContactFooter-success'
                              {% endif %}
                              required
                            {% endcapture %}
                      {% render 'field-input', type: 'email', name: 'contact[email]', value: value, label: placeholder, attr: attr %}
                      <button
                        type='submit'
                        class='footer-v18__email-button'
                        name='commit'
                        id='Subscribe'
                        aria-label='{{ 'newsletter.button_label' | t }}'
                      >
                        <div class='footer-v18__icon-before'>
                          {% render 'icon', name: 'email', icon_size: '16', color: 'rgba(var(--color-background))' %}
                        </div>
                        <div class='footer-v18__icon-after'>
                          {% render 'icon', name: 'email', icon_size: '16', color: 'rgba(var(--color-background))' %}
                        </div>
                      </button>
                    </div>
                    {% render 'newsletter-form-message', form: form %}
                  {%- endform -%}
                {%- endif -%}
                {%- if block.settings.show_social -%}
                  <ul role='list' class='footer-v18__social list-unstyled'>
                    <li>
                      <a href='{{block.settings.social_link1}}' class='footer-v18__social-link' target='_blank'>{{ block.settings.social_icon1 }}</a>
                    </li>
                    <li>
                      <a href='{{block.settings.social_link2}}' class='footer-v18__social-link' target='_blank'>{{ block.settings.social_icon2 }}</a>
                    </li>
                    <li>
                      <a href='{{block.settings.social_link3}}' class='footer-v18__social-link' target='_blank'>{{ block.settings.social_icon3 }}</a>
                    </li>
                    <li>
                      <a href='{{block.settings.social_link4}}' class='footer-v18__social-link' target='_blank'>{{ block.settings.social_icon4 }}</a>
                    </li>
                  </ul>
                {%- endif -%}
              </xo-animate>
            {% when 'menu' %}
              <xo-animate xo-cascade style='--xs: 12; --md: 4; --lg: 3'>
                {%- if block.settings.heading != blank -%}
                  <h2 class='footer-v18__heading'>{{- block.settings.heading -}}</h2>
                {%- endif -%}
                {%- if block.settings.menu != blank -%}
                  <ul role='list' class='list-unstyled'>
                    {%- for link in block.settings.menu.links -%}
                      <li class='footer-v18__list-item'>
                        <a
                          href='{{ link.url }}'
                        >
                          {{ link.title | escape }}
                        </a>
                      </li>
                    {%- endfor -%}
                  </ul>
                {%- endif -%}
              </xo-animate>
            {% else %}
              <xo-animate xo-cascade style='--xs: 12; --md: 4; --lg: 3'>
                {%- if block.settings.heading != blank -%}
                  <h2 class='footer-v18__heading'>{{- block.settings.heading -}}</h2>
                {%- endif -%}
                <div class='footer-v18__subtext'>
                  {{ block.settings.subtext }}
                </div>
              </xo-animate>
          {% endcase %}
        {% endfor %}
      </xo-grid>
    </xo-container>
  </div>
  <xo-container {% render 'section-layout' %}>
    <div class='footer-v18__bottom'>
      <div class='footer-v18__copyright'>
        {% if section.settings.copyright != blank %}
          {{- section.settings.copyright -}}
        {% else %}
          <small class='footer-v18__copyright-content'>&copy; Copyright {{ 'now' | date: '%Y' }} | {{ shop.name | escape | link_to: routes.root_url -}}</small>
          <small class='footer-v18__copyright-content'> By <a class='footer-v18__copyright-shopilaunch' href='https://shopilaunch.com/'> ShopiLaunch.</a> </small>
          <small class='footer-v18__copyright-content footer-v18__copyright-content--black'>{{ powered_by_link }}.</small>
        {% endif %}
      </div>
      <div class='footer-v18__payment'>
        {% if section.settings.image_payment != blank %}
          {% render 'image', image: section.settings.image_payment %}
        {% endif %}
      </div>
    </div>
  </xo-container>
</footer>

