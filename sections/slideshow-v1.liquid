

<section class='{{ 'xo-section--' | append: section.id }}'>
  <xo-carousel
    xo-render-bullet='<span></span>'
    xo-type='{{ section.settings.carousel_type }}'
    {% if section.settings.autoplay %}
      xo-autoplay='{{ section.settings.autoplay_speed | times: 1000 }}'
    {% endif %}
    xo-speed='{{ section.settings.changing_speed }}'
    xo-loop
    xo-hover-pause
  >
    <xo-carousel-inner>
      <xo-carousel-list role='list' aria-label='{{ 'general.slider.name' | t }}'>
        {% for block in section.blocks %}
          <xo-carousel-slide aria-roledescription='{{ 'sections.slideshow.slide' | t }}' aria-label='{{ forloop.index }} {{ 'general.slider.of' | t }} {{ forloop.length }}'>
            <div {% render 'slideshow-attr', block: block %}>
              {% render 'slideshow-bg-image', block: block %}
              {% if block.settings.background_type == 'image' and block.settings.background_image_desktop != blank %}
                {% comment %} Full-width banner with text overlay {% endcomment %}
                <div class='slideshow-v28__overlay-content' style='position: relative; z-index: 2; height: 100%; display: flex; align-items: center; justify-content: flex-start; min-height: {{ section.settings.section_height_desktop }}px;'>
                  <xo-container {% render 'section-layout' %}>
                    <div class='slideshow-v28__text slideshow-v28__text--overlay' style='text-align: left; max-width: 800px; margin: 0;'>
                      {%- capture subtitle -%}
                        {%- if block.settings.subtitle != blank -%}
                          <div class='slideshow-v28__subtitle' style='--fontsize:{{block.settings.subtitle_font_size}}; --fontweight:{{block.settings.subtitle_font_weight}}'>
                            {{ block.settings.subtitle }}
                          </div>
                        {%- endif -%}
                      {%- endcapture -%}
                      {%- capture title -%}
                        {%- if block.settings.title != blank -%}
                          <h3 class='slideshow-v28__title' style='--fontsize:{{block.settings.title_font_size}}; --fontweight:{{block.settings.title_font_weight}}'>
                            {{ block.settings.title }}
                          </h3>
                        {%- endif -%}
                      {%- endcapture -%}
                      {%- capture description -%}
                        {%- if block.settings.description != blank -%}
                          <p class='slideshow-v28__description' style='--fontsize:{{block.settings.description_font_size}}; --fontweight:{{block.settings.description_font_weight}}'>
                            {{ block.settings.description }}
                          </p>
                        {%- endif -%}
                      {%- endcapture -%}
                      {%- capture button -%}
                        {%- if block.settings.button_text != blank -%}
                          {% render 'button',
                            text: block.settings.button_text,
                            link: block.settings.button_link,
                            mode: block.settings.button_mode,
                            style: block.settings.button_style,
                            size: block.settings.button_size,
                            radius: block.settings.button_radius,
                            padding: block.settings.button_padding
                          %}
                        {%- endif -%}
                      {%- endcapture -%}

                      {%- render 'animation', content: subtitle, type: block.settings.subtitle_animation, order: 1, duration: '1400' -%}
                      {%- render 'animation', content: title, type: block.settings.title_animation, order: 3, duration: '1400' -%}
                      {%- render 'animation', content: description, type: block.settings.description_animation, order: 5, duration: '1400' -%}
                      {%- render 'animation', content: button, type: block.settings.button_animation, order: 7, duration: '1400', strength: 3 -%}
                    </div>
                  </xo-container>
                  {%- if section.settings.show_pagination -%}
                    {% render 'carousel-pagination-style3', class: 'slideshow-v28__pagination' %}
                  {%- endif -%}
                </div>
              {% else %}
                {% comment %} Original two-column layout {% endcomment %}
                <xo-container {% render 'section-layout' %}>
                  <xo-grid
                    style='
                      --height-desktop: {{ section.settings.section_height_desktop }};
                      --height-mobile: {{ section.settings.section_height_mobile }};
                      --pt: {{ section.settings.padding_top }};
                      --pb: {{ section.settings.padding_bottom }};
                    '
                    class='slideshow-v28__content'
                  >
                    {% if block.settings.reverse_layout == false %}
                      {% assign start_column = 8 %}
                      {% assign image_column = 2 %}
                      {% assign order = 0 %}
                    {% else %}
                      {% assign start_column = 2 %}
                      {% assign image_column = 7 %}
                      {% assign order = 1 %}
                    {% endif %}
                    <div style='--xs:12; --md:5; --start-md:{{image_column}}; --order-xs: 1; --order-md: {{ order }}; z-index:9'>
                      {%- capture image_desktop -%}
                        {%- if block.settings.image_desktop != blank -%}
                          {% render 'image', image: block.settings.image_desktop , lazyload: false %}
                        {%- endif -%}
                      {%- endcapture -%}
                      {%- capture image_mobile -%}
                        {%- if block.settings.image_mobile != blank -%}
                          {% render 'image', image: block.settings.image_mobile , lazyload: false %}
                        {%- endif -%}
                      {%- endcapture -%}
                      {%- render 'animation',
                        content: image_desktop,
                        type: block.settings.image_desktop_animation,
                        order: 1,
                        duration: '2000',
                        class: 'slideshow-v28__img-desktop'
                      -%}
                      {%- render 'animation', content: image_mobile, type: block.settings.image_mobile_animation, order: 1, duration: '1000', class: 'slideshow-v28__img-mobile' -%}
                    </div>
                    <div style='--xs: 12;--md: 5; --start-md:{{start_column}}' class='slideshow-v28__text'>
                      {%- capture subtitle -%}
                        {%- if block.settings.subtitle != blank -%}
                          <div class='slideshow-v28__subtitle' style='--fontsize:{{block.settings.subtitle_font_size}}; --fontweight:{{block.settings.subtitle_font_weight}}'>
                            {{ block.settings.subtitle }}
                          </div>
                        {%- endif -%}
                      {%- endcapture -%}
                      {%- capture title -%}
                        {%- if block.settings.title != blank -%}
                          <h3 class='slideshow-v28__title' style='--fontsize:{{block.settings.title_font_size}}; --fontweight:{{block.settings.title_font_weight}}'>
                            {{ block.settings.title }}
                          </h3>
                        {%- endif -%}
                      {%- endcapture -%}
                      {%- capture description -%}
                        {%- if block.settings.description != blank -%}
                          <p class='slideshow-v28__description' style='--fontsize:{{block.settings.description_font_size}}; --fontweight:{{block.settings.description_font_weight}}'>
                            {{ block.settings.description }}
                          </p>
                        {%- endif -%}
                      {%- endcapture -%}
                      {%- capture button -%}
                        {%- if block.settings.button_text != blank -%}
                          {% render 'button',
                            text: block.settings.button_text,
                            link: block.settings.button_link,
                            mode: block.settings.button_mode,
                            style: block.settings.button_style,
                            size: block.settings.button_size,
                            radius: block.settings.button_radius,
                            padding: block.settings.button_padding
                          %}
                        {%- endif -%}
                      {%- endcapture -%}

                      {%- render 'animation', content: subtitle, type: block.settings.subtitle_animation, order: 1, duration: '1400' -%}
                      {%- render 'animation', content: title, type: block.settings.title_animation, order: 3, duration: '1400' -%}
                      {%- render 'animation', content: description, type: block.settings.description_animation, order: 5, duration: '1400' -%}
                      {%- render 'animation', content: button, type: block.settings.button_animation, order: 7, duration: '1400', strength: 3 -%}
                    </div>

                    {%- if section.settings.show_pagination -%}
                      {% render 'carousel-pagination-style3', class: 'slideshow-v28__pagination' %}
                    {%- endif -%}
                  </xo-grid>
                </xo-container>
              {% endif %}
            </div>
          </xo-carousel-slide>
        {% endfor %}
      </xo-carousel-list>
    </xo-carousel-inner>
  </xo-carousel>
</section>

